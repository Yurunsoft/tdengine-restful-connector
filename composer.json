{
    "name": "yurunsoft/tdengine-restful-connector",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "yurunsoft/yurun-http": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7",
        "friendsofphp/php-cs-fixer": "2.18.3",
        "phpstan/phpstan": "0.12.82"
    },
    "autoload": {
        "psr-4": {
            "Yurun\\TDEngine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yurun\\TDEngine\\Test\\": "tests/unit"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c ./tests/phpunit.xml"
    }
}
